{
  "name": "temporal-worker-versioning",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc --build",
    "worker1": "ts-node src/workerV1.ts",
    "worker2": "ts-node src/workerV2.ts",
    "worker21": "ts-node src/workerV21.ts",
    "sender": "ts-node src/client.ts",
    "finisher": "ts-node src/client-finish.ts"
  },
  "nodemonConfig": {
    "execMap": {
      "ts": "ts-node"
    },
    "ext": "ts",
    "watch": [
      "src"
    ]
  },
  "dependencies": {
    "@temporalio/activity": "1.8.0",
    "@temporalio/client": "1.8.0",
    "@temporalio/worker": "1.8.0",
    "@temporalio/workflow": "1.8.0",
    "nanoid": "3.x"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.0",
    "@types/node": "^16.11.43",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-deprecation": "^1.2.1",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "ts-node": "^10.8.1",
    "typescript": "^4.4.2"
  }
}
