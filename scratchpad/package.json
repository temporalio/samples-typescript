{
  "name": "temporal-scratchpad",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint .",
    "start": "ts-node scratchpad.ts",
    "start.watch": "nodemon scratchpad.ts"
  },
  "nodemonConfig": {
    "execMap": {
      "ts": "ts-node"
    },
    "ext": "ts",
    "watch": [
      "scratchpad.ts"
    ]
  },
  "dependencies": {
    "@temporalio/activity": "^1.11.5",
    "@temporalio/client": "^1.11.5",
    "@temporalio/worker": "^1.11.5",
    "@temporalio/workflow": "^1.11.5"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.4",
    "@types/node": "^22.9.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-deprecation": "^1.2.1",
    "nodemon": "^2.0.12",
    "prettier": "^2.8.8",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  }
}
